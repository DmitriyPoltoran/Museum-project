{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Museum service",
    "version" : "0.1.0"
  },
  "servers" : [ {
    "url" : "http://localhost:10010/api/v1"
  } ],
  "paths" : {
    "/timetable" : {
      "get" : {
        "description" : "Returns list of city events",
        "operationId" : "timetable",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "Filtering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/TimetableResponse"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "timetable-controller"
    },
    "/souvenirs" : {
      "get" : {
        "description" : "Returns list of city events",
        "operationId" : "souvenirs",
        "parameters" : [ {
          "name" : "date",
          "in" : "query",
          "description" : "Filtering date",
          "schema" : {
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Success",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/SouvenirsResponse"
                }
              }
            }
          },
          "default" : {
            "description" : "Error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/ErrorResponse"
                }
              }
            }
          }
        }
      },
      "x-swagger-router-controller" : "souvenirs-controller"
    },
    "/swagger" : {
      "x-swagger-pipe" : "swagger_raw"
    }
  },
  "components" : {
    "schemas" : {
      "TimetableResponse" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/Timetable"
        }
      },
      "SouvenirsResponse" : {
        "type" : "array",
        "items" : {
          "$ref" : "#/components/schemas/Souvenirs"
        }
      },
      "Timetable" : {
        "required" : [ "img" ],
        "type" : "object",
        "properties" : {
          "img" : {
            "type" : "string"
          },
          "price" : {
            "type" : "string"
          },
          "name" : {
            "type" : "string"
          },
          "date" : {
            "type" : "string"
          }
        }
      },
      "Souvenirs" : {
        "required" : [ "img" ],
        "type" : "object",
        "properties" : {
          "img" : {
            "type" : "string"
          },
          "price" : {
            "type" : "string"
          },
          "name" : {
            "type" : "string"
          }
        }
      },
      "ErrorResponse" : {
        "required" : [ "message" ],
        "type" : "object",
        "properties" : {
          "message" : {
            "type" : "string"
          }
        }
      }
    }
  }
}